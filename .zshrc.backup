PROMPT='%m:%C ~ '

export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting
source ~/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

export JDK_HOME=$(/usr/libexec/java_home -v 1.8 )
export ANDROID_HOME=/Users/user/Library/Android/sdk
export PATH=$ANDROID_HOME/tools:$PATH
export PATH=$ANDROID_HOME/platform-tools:$PATH

# GO
export PATH=/Users/user/Downloads/go_appengine:$PATH
export GOROOT=/usr/local/go
export GOPATH=/Users/user/Documents/GoLang
export GOOS=darwin
export GOARCH=amd64
export GOBIN=$GOROOT/bin
export PATH=$PATH:$GOBIN

#Terminal
alias t='cd /Users/user/Desktop && sh 2.sh' 

#CD
alias wk='open -a iTerm /Users/user/Desktop/WorkProjects'


#Git
alias gzn='git config --global user.name "yla@zwenex.com" && git config user.name' 
alias gyla='git config --global user.name "whiteteenboy@gmail.com" && git config user.name' 


[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="/Users/user/.sdkman"
[[ -s "/Users/user/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/user/.sdkman/bin/sdkman-init.sh"
